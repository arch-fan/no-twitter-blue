{

    "manifest_version": 2,
    "name": "NoTwitterBlue",
    "version": "1.0.0",
  
    "description": "Deletes all tweets from accounts with twitter blue",
  
    "icons": {
      "96": "icons/logo.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://twitter.com/*"],
        "js": ["src/jquery-3.6.4.min.js", "src/tweets.js"]
      }
    ],

    "browser_action": {
      "default_popup": "popup/index.html"
    }
  
}
  